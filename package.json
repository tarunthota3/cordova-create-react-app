{
  "name": "cordova-create-react-app",
  "displayName": "HelloWorld",
  "version": "1.0.3",
  "description": "A sample Apache Cordova application with react application that responds to the deviceready event.",
  "main": "index.js",
  "homepage": "https://github.com/tarunthota3/cordova-create-react-app",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "android": "cordova run android",
    "desktop": "cordova run electron --nobuild"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tarunthota3/cordova-create-react-app.git"
  },
  "author": "Tarun Thota",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "rimraf": "^3.0.2",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "cordova-android": "^9.0.0",
    "cordova-electron": "^1.1.1",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-whitelist": "^1.3.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "ios",
      "android",
      "electron"
    ]
  }
}